  <body>
    <style>
      .modal-footer button:last-child {
        margin-right: 0 !important;
      }

      .top-nav-custom {
        flex: 1;
      }

      .top-nav-custom:first-child > span {
        margin-right: auto;
      }

      .top-nav-custom:last-child > span {
        margin-left: auto;
      }

      .modal-footer > * {
        margin-left: 0 !important;
        margin-right: 0 !important;
      }

      .dataTables_paginate {
        display: flex;
        justify-content: end;
        padding-top: 15px;
        padding-right: 15px;
      }

      .dz-button {
        font-weight: 300;
        font-size: 0.5em;
      }

      .avatar {
        display: flex;
        border: 1px solid lightgray;
        border-radius: 100%;
        align-items: center;
        overflow: hidden;
      }

      .avatar img {
        height: fit-content;
      }
    </style>
    <!-- Layout wrapper -->
    <div class="layout-wrapper layout-navbar-full layout-horizontal layout-without-menu">
      <div class="layout-container">
        <!-- Navbar -->

        <nav
          style="height: auto"
          class="layout-navbar navbar navbar-expand-xl align-items-center justify-content-between bg-navbar-theme pt-1"
          id="layout-navbar">
          <div class="container-fluid gap-3">
            <div class="top-nav-custom d-flex align-items-center gap-2">
              <h4 id="datetime" class="mb-0">-</h4>
            </div>
            <div class="text-center">
              <h4 class="mb-0">Verificador</h4>
            </div>

            <div class="d-flex align-items-center top-nav-custom" id="navbar-collapse">
              <ul class="navbar-nav flex-row align-items-center ms-auto">
                <!-- User -->
                <li class="nav-item navbar-dropdown dropdown-user dropdown">
                  <button id="exit-checker" type="button" class="btn btn-label-danger">
                    <span class="tf-icons ti-xs ti ti-logout me-1"></span>Salir
                  </button>
                </li>
                <!--/ User -->
              </ul>
            </div>
          </div>
        </nav>

        <!-- / Navbar -->

        <!-- Layout container -->
        <div class="layout-page">
          <!-- Content wrapper -->
          <div class="content-wrapper">
            <div class="container-fluid flex-grow-1 container-p-y"><%- body %></div>
            <!--/ Content -->

            <!-- Footer -->
            <footer class="content-footer footer bg-footer-theme">
              <div class="container-fluid">
                <div
                  class="footer-container d-flex align-items-center justify-content-between py-2 flex-md-row flex-column">
                  <div>
                    ©
                    <script>
                      document.write(new Date().getFullYear());
                    </script>
                    OTV - Impulsora
                  </div>
                  <div class="d-none d-lg-inline-block">
                    <a href="https://tecsom.mx" target="_blank" class="footer-link">Desarrollado por Tecsom</a>
                  </div>
                </div>
              </div>
            </footer>
            <!-- / Footer -->

            <div class="content-backdrop fade"></div>
          </div>
          <!--/ Content wrapper -->
        </div>

        <!--/ Layout container -->
      </div>
    </div>

    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>

    <!-- Drag Target Area To SlideIn Menu On Small Screens -->
    <div class="drag-target"></div>

    <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->

    <script src="/public/assets/vendor/libs/popper/popper.js"></script>
    <script src="/public/assets/vendor/js/bootstrap.js"></script>
